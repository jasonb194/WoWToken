{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": null,
  "installCommand": "pnpm install",
  "functions": {
    "api/**/*.ts": {
      "maxDuration": 300
    }
  },
  "routes": [
    {
      "src": "/api/check-prices",
      "dest": "/api/check-prices.ts"
    },
    {
      "src": "/api/interactions",
      "dest": "/api/interactions.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    }
  ]
} 