{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "maxDuration": 300
    }
  },
  "routes": [
    {
      "src": "/api/check-prices",
      "dest": "/api/check-prices.js"
    },
    {
      "src": "/api/interactions",
      "dest": "/api/interactions.js"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    }
  ]
} 